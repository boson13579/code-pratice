sturct node{
    node *left=NULL, *mid=NULL, *right=NULL;
    int fre;
}

TernaryHuffman(C)
    Q <- C          //Q: priority queue
    if Q.size()%2 == 0 
        Q.insert(new node(fre = 0)) //如果Q的大小是大於1的偶數，那麼加上一個fre是0 的 node 確保3元數的個數
    while(Q.size() > 1)
        x = Extract-Min(Q)
        y = Extract-Min(Q)
        z = Extract-Min(Q)
        temp = new mode(left = x, mid = y, right = z, fre = x.fre + y.fre + z.fre)
        Q.insert(temp)
    return Extract-Min(Q)

Time complixity O(nlogn)